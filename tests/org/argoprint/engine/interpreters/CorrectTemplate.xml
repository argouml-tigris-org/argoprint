<root>
    <!-- Test tag for default. -->
    <default_test>
        Tjohoj!!
    </default_test>

    <!-- Test tag for Call. -->
    <collect_call>
        <ap:call what="getString()"/>
        </collect_call>
    <expected_ap_call_result>
        <collect_call>
            Return value of getString() in DataSourceStub.
        </collect_call>
    </expected_ap_call_result>

    <!-- Test tag for Iterate. -->
    <collect_iterate>
        <ap:iterate what="getCollection()" iteratorname="it">
            Tjoho.
        </ap:iterate>
    </collect_iterate>
    <collect_iterate>
        <ap:iterate what="getCollection()" iteratorname="it" sortvalue="intern">
            <ap:call what="toString" iterator="it"/>
        </ap:iterate>
    </collect_iterate>
    <expected_ap_iterate_result>
        <collect_iterate>
            Tjoho.
            Tjoho.
            Tjoho.
            Tjoho.
            Tjoho.
        </collect_iterate>
        <collect_iterate>
            object1
            object2
            object3
            object4
            object5
        </collect_iterate>
    </expected_ap_iterate_result>

    <!-- Test tag for If. -->
    <collect_if>
        <ap:if cond="getTrueBoolean()">
            <ap:then>
                Correct!!
            </ap:then>
            <ap:else>
                Incorrect!
            </ap:else>
        </ap:if>
    </collect_if>
    <expected_ap_if_result>
        <collect_if>
            Correct!!
        </collect_if>
    </expected_ap_if_result>

    <!-- Test tag for Bind. -->
    <collect_bind>
        <ap:bind name="img">
            <ap:attr name="src">Hoj</ap:attr>
            image
        </ap:bind>
    </collect_bind>
    <expected_ap_bind_result>
        <collect_bind>
            <img src="Hoj">image</img>
        </collect_bind>
    </expected_ap_bind_result>
</root>
